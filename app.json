{
  "expo": {
    "name": "Advanced Todo App",
    "slug": "advanced-todo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/toto-v2.png",
    "scheme": "advancedtodoapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/toto-v2.png",
        "backgroundImage": "./assets/toto-v2.png",
        "monochromeImage": "./assets/toto-v2.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ripassorkerrifat.advancedtodoapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/toto-v2.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/toto-v2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2dbc93f0-1cfa-4283-bbcc-2078f961e9ce"
      }
    }
  }
}
